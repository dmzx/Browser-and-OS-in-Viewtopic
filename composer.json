{
	"name": "dmzx/browsericon",
	"type": "phpbb-extension",
	"description": "This extension shows browser icon and OS icon in viewtopic.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.2",
	"time": "2019-11-04",
	"keywords": [
		"phpbb",
		"extension",
		"browsericon"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Browser & OS in Viewtopic",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.3-RC1,<4.0.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "browsericon_version.json",
			"ssl": true
		}
	}
